/* pages/page/pages.wxss */
page{
background-color:#f1f1f1;
}
.container{
  position: relative;
  width: 100%;
  height: 528px;
  display: flex;
  flex-direction:column;
  align-items: center;
  
  
}
.user{
  position: relative;
  display: flex;
  width: 100%;
  height: 30%;
  background-color: #fff;
  padding:5px; 
}
.navigator{
  height: 50px;
  width:100%;
  margin-bottom: 5px
}
.more{
  position: relative;
  height: 60%;
  width: 100%;
  display: flex;
  margin-top: 5px;
  flex-direction:column;
  align-items: center;
}
.user-login{
  position: relative;
  display: flex;
  
  flex-direction:column;
  align-items: center;
  
  width: 100%;
}
.user-unlogin{
  position: relative;
  display: flex;
  
  flex-direction:column;
  align-items: center;
  
  width: 100%;
  
}
.unlogin{
  width: 80px;
  height: 80px;
}
.loginbutton{

  width: 150px;
  height: 30px;
}
.userImg{
  height: 80px;
  width: 80px;
  border-radius: 50%
}
.enroll{
  position: relative;
  height: 50px;
  width: 100%;
  display: flex;
  justify-content:space-between;
  align-items: center;
  background-color: #fff; 
  border-right: 5px;
  border-left: 5px;
}
.arrowright{
  height: 20px;
  width: 20px;
  padding-right: 5px;
}